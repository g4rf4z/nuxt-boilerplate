{
  "name": "nuxt-app",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.11.1",
  "description": "A modular, production-ready Nuxt boilerplate designed to quickly launch any kind of project with minimal configuration",
  "author": "Gr√©goire Pinte",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:generate": "drizzle-kit generate",
    "deps:clear": "rm -rf node_modules .nuxt",
    "deps:reset": "pnpm deps:clear && pnpm install && pnpm postinstall",
    "cleaninstall": "rm -rf pnpm-lock.yaml && pnpm deps:reset"
  },
  "dependencies": {
    "@nuxt/content": "3.5.1",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/icon": "1.13.0",
    "@nuxt/image": "1.10.0",
    "@nuxt/ui": "3.1.3",
    "@nuxthub/core": "0.8.27",
    "@nuxtjs/i18n": "9.5.5",
    "@pinia/colada": "0.17.0",
    "@pinia/colada-nuxt": "0.2.1",
    "@pinia/nuxt": "0.11.1",
    "@vueuse/nuxt": "13.3.0",
    "nuxt": "3.17.5",
    "nuxt-auth-utils": "0.5.20",
    "nuxt-viewport": "2.3.1",
    "pinia": "3.0.3",
    "vue": "3.5.16",
    "vue-router": "4.5.1",
    "zod": "3.25.51"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.13.3",
    "@nuxt/eslint": "1.4.1",
    "drizzle-kit": "0.31.1",
    "eslint": "9.28.0",
    "typescript": "5.8.3",
    "vue-tsc": "2.2.10",
    "wrangler": "4.19.1"
  }
}
